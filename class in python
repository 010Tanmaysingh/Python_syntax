class computer:
    def __init__(self,cpu,ram):
        self.cpu=cpu
        self.ram=ram
    def config(self):
        print('config is',self.cpu,self.ram)

comp1=computer('i5',8,) #these parameter in brackets get passed to the __init__(self, in cpu,and in ram)
comp2=computer('ryzen7',16)
comp1.config()
comp2.config()
